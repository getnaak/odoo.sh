<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<data noupdate="0">

        <record id="base.main_company" model="res.company">
            <field name="name">Naak</field>
			<field name="email">info@naak.io</field>
			<field name="phone">310-263-8331</field>
			<field name="street">183 E City Place Dr</field>
            <field name="city">Santa Ana</field>
            <field name="zip">92705</field>
            <field name="state_id" ref="base.state_us_5"/>
			<field name="country_id" ref="base.us"/>
			<field name="logo" type="base64" file="naak_website/static/img/naak_logo.png"/>
		</record>

        <record id="website.main_menu" model="website.menu">
          <field name="name">Default Main Menu</field>
          <field name="url">/default-main-menu</field>
		  <field name="website_id" ref="website.default_website"/>
        </record>

        <record id="menu_partner_spt" model="website.menu">
            <field name="name">Partner</field>
            <field name="url">/partners</field>
            <field name="sequence">12</field>
            <field name="parent_id" ref="website.main_menu"/>
            <field name="website_id" ref="website.default_website" />
        </record>

	</data>

</odoo>